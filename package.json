{
    "name": "monotropic-theme",
    "version": "0.2.3",
    "displayName": "monotropic",
    "homepage": "https://github.com/avelino/monotropic-theme-vscode",
    "description": "(Almost) monochromatic theme for vscode",
    "publisher": "avelino",
    "license": "MIT",
    "scripts": {
        "build": "nbb -m scripts.generate",
        "package": "mkdir -p bin && vsce package -o ./bin/monotropic.vsix",
        "test": "nbb -m test.generate",
        "vscode:prepublish": "nbb -m scripts.generate",
        "vsce-publish": "vsce publish"
    },
    "maintainers": [
        "Thiago Avelino <avelinorun@gmail.com>"
    ],
    "contributors": [
        "Rodrigo Franco",
        "Leo Aparisi de Lannoy"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/avelino/monotropic-theme-vscode.git"
    },
    "bugs": {
        "url": "https://github.com/avelino/monotropic-theme-vscode/issues"
    },
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Themes"
    ],
    "keywords": [
        "monotropic",
        "monochrome",
        "light",
        "theme",
        "color-theme"
    ],
    "icon": "assets/monotropic.png",
    "galleryBanner": {
        "color": "#fffffa",
        "theme": "light"
    },
    "contributes": {
        "themes": [
            {
                "label": "monotropic",
                "uiTheme": "vs",
                "path": "./theme/monotropic.json"
            },
            {
                "label": "monotropic-coffee",
                "uiTheme": "vs",
                "path": "./theme/monotropic-coffee.json"
            },
            {
                "label": "monotropic-gradient",
                "uiTheme": "vs",
                "path": "./theme/monotropic-gradient.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^4.1.1",
        "prettier": "^2.8.8",
        "tinycolor2": "^1.6.0",
        "@vscode/vsce": "^2.19.0"
    },
    "__metadata": {
        "id": "e9a92eb4-ebc4-455e-9a80-a9666d03ee86",
        "publisherDisplayName": "avelino",
        "publisherId": "6c7da285-e398-41d7-a76c-9e13c3c5e16e"
    },
    "dependencies": {
        "nbb": "^1.2.174"
    }
}
